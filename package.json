    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "ioredis": "^5.4.0",
    "openai": "^4.70.0",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
